---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <url>
    <loc>http://ronin.rubyforge.org/</loc>
    <lastmod>{{ site.time | date_to_xmlschema}}</lastmod>
    <changefreq>yearly</changefreq>
    <priority>1.0</priority>
  </url>
  <url>
    <loc>http://ronin.rubyforge.org/blog/</loc>
    <lastmod>{{ site.time | date_to_xmlschema}}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.9</priority>
  </url>
  <url>
    <loc>http://ronin.rubyforge.org/faq.html</loc>
    <lastmod>{{ site.time | date_to_xmlschema}}</lastmod>
    <changefreq>yearly</changefreq>
    <priority>0.8</priority>
  </url>
  <url>
    <loc>http://ronin.rubyforge.org/resources/</loc>
    <lastmod>{{ site.time | date_to_xmlschema}}</lastmod>
    <changefreq>yearly</changefreq>
    <priority>0.7</priority>
  </url>
  <url>
    <loc>http://ronin.rubyforge.org/downloads/</loc>
    <lastmod>{{ site.time | date_to_xmlschema}}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.6</priority>
  </url>
  <url>
    <loc>http://ronin.rubyforge.org/contribute/</loc>
    <lastmod>{{ site.time | date_to_xmlschema}}</lastmod>
    <changefreq>yearly</changefreq>
    <priority>0.4</priority>
  </url>
  <url>
    <loc>http://ronin.rubyforge.org/contact.html</loc>
    <lastmod>{{ site.time | date_to_xmlschema}}</lastmod>
    <changefreq>yearly</changefreq>
    <priority>0.3</priority>
  </url>
  {% for post in site.categories.blog %}
  <url>
    <loc>http://ronin.rubyforge.org{{ post.url }}</loc>
    <lastmod>{{ post.date | date_to_xmlschema }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.5</priority>
  </url>
  {% endfor %}
  <url>
    <loc>http://ronin.rubyforge.org/examples/</loc>
    <lastmod>{{ post.date | date_to_xmlschema }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.1</priority>
  </url>
  <url>
    <loc>http://ronin.rubyforge.org/examples/commands/</loc>
    <lastmod>{{ post.date | date_to_xmlschema }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.1</priority>
  </url>
  <url>
    <loc>http://ronin.rubyforge.org/examples/formatting/</loc>
    <lastmod>{{ post.date | date_to_xmlschema }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.1</priority>
  </url>
  <url>
    <loc>http://ronin.rubyforge.org/examples/network/</loc>
    <lastmod>{{ post.date | date_to_xmlschema }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.1</priority>
  </url>
  <url>
    <loc>http://ronin.rubyforge.org/examples/web/</loc>
    <lastmod>{{ post.date | date_to_xmlschema }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.1</priority>
  </url>
</urlset>
